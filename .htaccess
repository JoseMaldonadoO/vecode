<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /VECODE/

    # Redirigir todo a la carpeta public
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ public/$1 [L]

    # Redirigir la ra√≠z a public
    RewriteRule ^$ public/ [L]
</IfModule>
